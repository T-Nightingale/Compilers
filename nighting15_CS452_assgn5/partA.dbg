244#	beq $t2,$zero,foundNewLine
248#	sw $v0,lastCharRead # push back character into queue
252#	j foundNewLine # go process the line

256:  notReturnRL:

	# at this point, $v0 has our character

256#	subu $t0,$v0,10
260#	beq $t0,$zero,foundNewLine
264#	blt $v0,$zero,foundEof
265##...slt $at,$v0,$zero
269##...bne $at,$zero,$+16

	# we have a character, so push it onto stack
272#	subu $sp,$sp,4
276#	sw $v0,($sp)

