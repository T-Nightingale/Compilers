#    - ($sp) divisor
#    - 4($sp) (dummy) GC tag
#    - 8($sp) dividend
#    - 12($sp) (dummy) GC tag
#  - return value:
#    - ($sp) result
#    - 4($sp) (dummy) GC tag
########################################################
2432:remainder:
	# get parameters; abort if divisor zero
2432#	lw $t0,($sp)
2436#	lw $t1,8($sp)
2440#	beq $t0,$zero,divByZeroError
	
	# perform division
2444#	div $t1,$t0
2448#	mfhi $t0

	# store result, adjust stack and return
2452#	addu $sp,$sp,8 # adjust stack
2456#	sw $t0,($sp) # store result
2460#	jr $ra
	
########################################################
# checkCast (millicode) - checks that a cast is legal
#  - aborts if null
#  - aborts if cast is illegal cast
#  - parameters:
#    - ($sp) object to check
#    - $t0 address of vtable for target-class
#    - $t1 address one past vtable address of last
#      subclass of target-class
#  - return value:
#    - ($sp) object to check (now checked)
#  - side-effects: clobbers $t2 and $t3
########################################################
2464:checkCast:
	# get object, allow immediately if null
2464#	lw $t2,($sp)
2468#	beq $t2,$zero,checkCastReturn
	
	# get vtable address of object (using Object vtable
	# address for arrays)
2472#	lw $t3,-8($t2) # <= 0 if array
2476#	lw $t2,-12($t2) # vtable address (unless array)
2480#	bgt $t3,$zero,skipArrayCast
2481##...slt $at,$zero,$t3
2485##...bne $at,$zero,$+8
2488#	la $t2,CLASS_Object
	
	# get vtable address of object, abort if less than
	# lower limit or greater then or equal to higher
	# limit
2492:  skipArrayCast:
2492#	bge $t2,$t1,castException
2493##...slt $at,$t2,$t1
2497##...beq $at,$zero,$+152
2500#	blt $t2,$t0,castException
2501##...slt $at,$t2,$t0
2505##...bne $at,$zero,$+144
	
	# return, leaving object unchanged on stack
2508:  checkCastReturn:	
2508#	jr $ra	
	
########################################################
# instanceOf (millicode) - tests whether an object is
#   a member of a given class (or subclass)
#  - returns false if object is null
#  - parameters:
#    - ($sp) object to check
#    - $t0 address of vtable for target-class
#    - $t1 address one past vtable address of last
#      subclass of target-class
#  - return value:
#    - ($sp) true (1) or false (0), depending on whether
#      object is a member
#  - side-effects: clobbers $t2 and $t3
########################################################
2512:instanceOf:
	# get object, go return false if null
2512#	lw $t2,($sp)
2516#	beq $t2,$zero,doneInstanceOf
	
	# get vtable address of object.  If it's an array
	# (which would be H1 <= 0), use vtable address for Object
2520#	lw $t3,-8($t2) # <= 0 if array
2524#	lw $t2,-12($t2) # vtable address (unless array)
2528#	bgt $t3,$zero,skipArrayInstanceOf
2529##...slt $at,$zero,$t3
2533##...bne $at,$zero,$+8
2536#	la $t2,CLASS_Object
	
	# get vtable address of object, abort if less than
	# lower limit or greater then or equal to higher
	# limit
2540:  skipArrayInstanceOf:
2540#	sge $t0,$t2,$t0
2541##...slt $t0,$t2,$t0
2545##...xori $t0,$t0,0x1
2548#	slt $t1,$t2,$t1
	
	# store the AND of the two conditions onto the stack; return
2552#	and $t2,$t0,$t1
2556:  doneInstanceOf: # if we reach here via branch, we know $t2=0
2556#	sw $t2,($sp)
2560#	jr $ra

